{
  "name": "tt-studio-frontend",
  "private": true,
  "version": "2.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . && npm run type-check",
    "lint:fix": "eslint . --fix && prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint:fix:errors": "eslint . --fix --quiet",
    "lint:fix:warnings": "eslint . --fix --max-warnings=0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "generate-license": "npx generate-license-file --input package.json --output ./third-party-licenses.txt --overwrite",
    "audit": "npm audit",
    "header": "npm run header:check",
    "header:check": "eslint .",
    "header:check:changed": "node scripts/check-headers-changed.js",
    "header:check:strict": "eslint . --max-warnings=0",
    "header:fix": "node scripts/add-headers.js",
    "header:fix:all": "node scripts/add-headers.js",
    "header:fix:changed": "node scripts/add-headers.js --changed-only",
    "header:fix:dry-run": "node scripts/add-headers.js --dry-run",
    "header:fix:verbose": "node scripts/add-headers.js --verbose",
    "header:test": "node test-header-checker.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tabler/icons-react": "^3.26.0",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.28.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.4",
    "compromise": "^14.14.4",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.344.0",
    "mini-svg-data-uri": "^1.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^2.1.7",
    "react-router-dom": "^6.28.0",
    "remark-gfm": "^4.0.0",
    "shiki": "^1.23.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^1.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-headers": "^1.3.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "generate-license-file": "^4.0.0",
    "globals": "^15.15.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.6.3",
    "vite": "^6.3.5"
  }
}
